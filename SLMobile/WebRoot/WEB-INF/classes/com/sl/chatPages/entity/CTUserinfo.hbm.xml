<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sl.chatPages.entity.CTUserinfo" table="CT_userinfo">

		<id name="uid" type="string">
			<column name="uid" />
		</id>
		<property name="uname" column="uname" type="string"></property>
		<property name="nickName" column="nickName" type="string"></property>
		<!-- <property name="head" column="head" type=""></property> -->
		<property name="photo" column="photo" type="string"></property>
		<property name="sex" column="sex" type="character"></property>
		<property name="age" column="age" type="integer"></property>
		<property name="lastLogin" column="lastLogin" type="time"></property>
		<property name="dr" column="dr" type="short"></property>
	</class>
</hibernate-mapping>
